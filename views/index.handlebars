<h2>Home</h2>


{{#each postagens as |postagens|}}

    <div class="card">
        <div class="card-body">
            
            
            <h3>{{titulo}}</h3>
            <p>{{descricao}}</p>
            <a href="/postagem/{{slug}}"><button class="btn btn-primary">Leia mais</button></a>
            <hr/>
            <small>{{categoria.nome}}</small>
            <small>Data: {{#formatDate date}}{{/formatDate}}</small>
        </div>
    </div>
{{/each}}